<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.TextArea?>
<AnchorPane maxHeight="170.0" maxWidth="170.0" minHeight="170.0" minWidth="170.0" prefHeight="170.0" prefWidth="170.0" style="-fx-background-color: #f9f9f9; -fx-border-color: #e0e0e0; -fx-border-width: 1px;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21"
            fx:controller="ma.enset.demo.controllers.Note_Controller">
    <children>

        <!-- Nom du chapitre -->
        <Label fx:id="chapName" layoutX="14.0" layoutY="14.0" text="Chapter Name"
               style="-fx-font-size: 12px; -fx-font-weight: bold; -fx-text-fill: #333333;" />

        <!-- NumÃ©ro de page -->
        <Label fx:id="pageNumber" layoutX="132.0" layoutY="148.0" text="Page 1"
               style="-fx-font-size: 10px; -fx-text-fill: #666666;" />

        <!-- Zone de texte pour la note -->
        <TextArea fx:id="noteText" layoutX="14.0" layoutY="31.0"
                  minHeight="113.0" minWidth="143.0" prefHeight="113.0" prefWidth="143.0"
                  style="-fx-background-color: #ffffff; -fx-border-color: #d4d4d4; -fx-border-width: 1px;" />

    </children>
</AnchorPane>
